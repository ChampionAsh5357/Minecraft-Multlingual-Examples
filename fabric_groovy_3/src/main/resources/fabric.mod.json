{
    "schemaVersion": 1,

    "name": "Multlingual Examples - Fabric Groovy",
    "id": "multilingual_examples_fabric_groovy",
    "version": "${version}",
    "description": "An example mod for Minecraft 1.17.x in the Groovy programming language via the Fabric Mod Loader.",

    "authors": [
        "ChampionAsh5357"
    ],
    "contact": {
        "homepage": "https://github.com/ChampionAsh5357/minecraft-multlingual-examples",
        "sources": "https://github.com/ChampionAsh5357/minecraft-multlingual-examples",
        "issues": "https://github.com/ChampionAsh5357/minecraft-multlingual-examples/issues"
    },
    "license": "CC0-1.0",

    "environment": "*",
    "entrypoints": {
        "main": [
            {
                "adapter": "groovy",
                "value": "net.ashwork.multilingualexamples.fabric.groovy.MultilingualExamples"
            }
        ]
    },

    "depends": {
        "fabricloader": ">=0.11.6",
        "fabric": "*",
        "minecraft": "1.17.x",
        "java": ">=16",
        "fabric-language-groovy": ">=1.1.0"
    }
}